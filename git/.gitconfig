[core]
	editor = vim
	whitespace = cr-at-eol
[alias]
	st = status
	br = branch
	ch = checkout
	co = commit
[color]
	ui = true
	diff = true
[include]
	path = ~/.gitconfig_local
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
